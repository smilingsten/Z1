<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  
    <servlet>
    <servlet-name>smsServlet</servlet-name>
    <servlet-class>de.sten.server.SMSServiceImpl2</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>smsServlet</servlet-name>
    <url-pattern>/z1/smsserv</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Z1.html</welcome-file>
  </welcome-file-list>

</web-app>
